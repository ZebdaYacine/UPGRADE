<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane id="AnchorPane" prefHeight="211.0" prefWidth="312.0" stylesheets="@../Styles/clientui.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="upgrade.UIController.AddNeedUIController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="394.0" minWidth="10.0" prefWidth="312.0" />
    </columnConstraints>
    <rowConstraints>
      <RowConstraints maxHeight="195.0" minHeight="10.0" prefHeight="65.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="195.0" minHeight="10.0" prefHeight="65.0" vgrow="SOMETIMES" />
        <RowConstraints maxHeight="342.0" minHeight="10.0" prefHeight="68.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <effect>
        <DropShadow height="30.0" radius="12.25" />
    </effect>
    <children>
        <JFXButton fx:id="addBtn" onAction="#addNeed" prefHeight="50.0" prefWidth="300.0" stylesheets="@../styles/clientui.css" text="Ajouter" GridPane.rowIndex="2">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
            <GridPane.margin>
                <Insets left="10.0" right="10.0" />
            </GridPane.margin>
           
        </JFXButton>
        <JFXComboBox fx:id="CmbOffice" prefHeight="33.0" prefWidth="280.0" promptText="Bureau">
            <GridPane.margin>
                <Insets left="10.0" right="10.0" />
            </GridPane.margin>
        </JFXComboBox>
        <JFXComboBox fx:id="CmbSp" prefHeight="33.0" prefWidth="280.0" promptText="Specialite" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets left="10.0" right="10.0" />
            </GridPane.margin>
        </JFXComboBox>
    </children>
</GridPane>
